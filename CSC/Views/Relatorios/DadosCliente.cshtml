<div class="container-fluid">
    <form method="post" target="_blank">
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Data de Cadastro :</h3>
            </div>
            <div class="card-body">
                <div class="form-row">
                    <div class="col-2">
                        <label class="col-form-label">De :</label>
                        <div id="datai" class="input-group date">
                            <input id="DataInicial" type="text" class="form-control form-control-sm" name="DataInicial" required />
                            <span class="input-group-addon" style="position:absolute; right:5px;"><i class="fa fa-calendar"></i></span>
                        </div>
                    </div>
                    <div class="col-2">
                        <label class="col-form-label">Até :</label>
                        <div id="dataf" class="input-group date">
                            <input id="DataFinal" type="text" class="form-control form-control-sm" name="DataFinal" required />
                            <span class="input-group-addon" style="position:absolute; right:5px;"><i class="fa fa-calendar"></i></span>
                        </div>
                    </div>
                    <div class="col-1">
                    </div>
                    <div class="col-3">
                        <div class="custom-control custom-radio">
                            <input type="radio" class="custom-control-input" id="ativo" name="status" value="0">
                            <label class="custom-control-label" for="ativo">Ativos</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input type="radio" class="custom-control-input" id="inativo" name="status" value="1" />
                            <label class="custom-control-label" for="inativo">Inativos</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input type="radio" class="custom-control-input" id="ambos" name="status" value="2" />
                            <label class="custom-control-label" for="ambos">Ambos</label>
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="col-2">
                        <label>&nbsp;</label>
                        <button type="button" id="btnPesquisar" class="btn btn-block btn-primary">Gerar</button>
                    </div>
                    <div class="col-2">
                        <label>&nbsp;</label>
                        <button type="submit" asp-action="DadosClientePDF" id="btnImprimir" class="btn btn-block btn-primary">Imprimir</button>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>
<div id="content">

</div>

@section Scripts{
    <script type="text/javascript" src="~/js/DadosCliente.js"></script>
}